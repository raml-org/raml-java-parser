#%RAML 1.0
title: simple object
types:
  root:
    properties:
      name: string
      friends: Friend[]
    additionalProperties: false
  Friend:
    type: object
    properties:
      name: string
    additionalProperties: true
/res:
  get:
    body:
      application/xml:
          type: root
          example: |
            <root>
              <name>robert</name>
              <friends>
                <name>denerys</name>
              </friends>
              <friends>
                <name>stannis</name>
              </friends>
              <friends>
                <name>arya</name>
                <my-custom-xml>with-custom-value</my-custom-xml>
              </friends>
            </root>