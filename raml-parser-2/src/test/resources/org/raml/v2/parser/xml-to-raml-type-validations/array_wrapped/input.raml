#%RAML 1.0
title: simple object
types:
  root:
    properties:
      name: string
      friends:
        type: Friend[]
        xml:
          wrapped: true
    additionalProperties: false
  Friend:
    type: object
    properties:
      name: string
/res:
  get:
    body:
      application/xml:
          type: root
          example: |
            <root>
              <name>robert</name>
              <friends>
                <Friend>
                  <name>daenerys</name>
                </Friend>
                <Friend>
                  <name>stannis</name>
                </Friend>
              </friends>
            </root>